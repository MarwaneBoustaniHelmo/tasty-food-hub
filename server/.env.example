# Express Server Configuration
API_PORT=3001
NODE_ENV=development

# Anthropic API (required for streaming)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Price Comparison API Settings
API_TIMEOUT_MS=5000
API_RETRY_COUNT=2

# Optional: Delivery Platform API Keys (if required)
# Most platforms don't require keys for basic search, but add if needed
UBER_EATS_API_KEY=your_key_here
DELIVEROO_API_KEY=your_key_here
TAKEAWAY_API_KEY=your_key_here

# Optional: OpenAI for embeddings
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Supabase for database
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_key
